{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:vue/vue3-recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "vue/multi-word-component-names": "off",
    "vue/no-setup-props-destructure": "off"
  },
  "overrides": [
    {
      "files": ["*.vue"],
      "rules": {
        "vue/no-setup-props-destructure": "off"
      }
    }
  ]
}
